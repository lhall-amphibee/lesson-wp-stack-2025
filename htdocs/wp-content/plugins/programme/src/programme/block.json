{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/programme",
	"version": "0.1.0",
	"title": "Event Program",
	"category": "widgets",
	"icon": "calendar",
	"description": "Display an event program with sessions and schedule",
	"example": {},
	"supports": {
		"html": false,
		"className": true,
		"styles": true
	},
	"textdomain": "programme",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"render": "file:./render.php",
	"attributes": {
		"title": {
			"type": "string",
			"default": ""
		},
		"date": {
			"type": "string",
			"default": ""
		},
		"location": {
			"type": "string",
			"default": ""
		},
		"sessions": {
			"type": "array",
			"default": []
		}
	}
}
